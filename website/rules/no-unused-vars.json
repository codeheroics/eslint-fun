{
  "name": "no-unused-vars",
  "description": "disallow declaration of variables that are not used in the code",
  "isRecommended": true,
  "isFixable": false,
  "category": "Variables",
  "schema": [
    {
      "oneOf": [
        {
          "enum": [
            "all",
            "local"
          ]
        },
        {
          "type": "object",
          "properties": {
            "vars": {
              "enum": [
                "all",
                "local"
              ]
            },
            "varsIgnorePattern": {
              "type": "string"
            },
            "args": {
              "enum": [
                "all",
                "after-used",
                "none"
              ]
            },
            "argsIgnorePattern": {
              "type": "string"
            }
          }
        }
      ]
    }
  ]
}